class Myclass:
    def __init__(self, name, age):
        self.name = name
        self.age = age
    def greet(self):
        return f"Hello, my name is {self.name} and I am {self.age} years old"
#both the functions are stored in class

k = Myclass("kabish", "17")
#this is the function call for Myclass function parameters are as name = "kabish and age = "17"
print(k.name) 
print(k.age)
print(k.greet())
